@import "_variable.styl"
@import "_background.styl"
@import "_menu.styl"
@import "_contactus.styl"
*
  // border 0.03vw solid red
  font-family 'Open Sans', sans-serif
  box-sizing border-box
body
  margin 0
  display grid
  justify-content center
  align-items center
.initsec
  display grid
  // background-color lightblue
  height 100vh
  justify-content center
  align-content center
.WelInit_ms_01
  // background-color blue
  color white
  font-size 40px
.prodsec
  padding-top 4em
  height 100vh
  display grid
  grid-template-columns auto auto auto auto auto auto
  grid-template-rows auto auto
  justify-content center
  align-content center
  color white
  font-weight bold
  @media screen and (max-width: 840px){
    padding-top (MenuRomH_ms * 0.7) - 1.5em
  }
  @media screen and (max-width: 760px){
    padding-top 4.5em
    z-index: 2
  }
  @media screen and (max-width: 400px){
    padding-top 3em
  }
.probloq_ms_01,.probloq_ms_02
  width (ProdBloq_ms * 1.5)
  height (ProdBloq_ms * 1.5)
  display grid
  justify-content center
  align-content center
.probloq_ms_03,.probloq_ms_04,.probloq_ms_05
  width ProdBloq_ms
  height ProdBloq_ms
  display grid
  justify-content center
  align-content center
.probloq_ms_01
  background-color blue
  grid-column-start 1
  grid-column-end 4
  grid-row-start 1
  grid-row-end 2
.probloq_ms_02
  background-color red
  grid-column-start 4
  grid-column-end 7
  grid-row-start 1
  grid-row-end 2
.probloq_ms_03
  background-color gray
  grid-column-start 1
  grid-column-end 3
  grid-row-start 2
  grid-row-end 3
.probloq_ms_04
  background-color yellow
  grid-column-start 3
  grid-column-end 5
  grid-row-start 2
  grid-row-end 3
.probloq_ms_05
  background-color pink
  grid-column-start 5
  grid-column-end 7
  grid-row-start 2
  grid-row-end 3
.empsec
  background-color green
  height 100vh
