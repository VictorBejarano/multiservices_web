.bmenu_ms_01
  display: none
  @media screen and (max-width: rSM_ms_w)
  {
    border 0.03vw solid red
    background-color blue
    display block
    z-index 9999
  }
.menu_ms_01
  width: 100%
  position: fixed
  height: 4vw
  top: 0
  background-color: rgba(0.3,0.3,0.3,0.8)
  margin: 0
  display: grid
  grid-template-columns: logo_ms menu_ms
  grid-template-rows: auto
  @media screen and (max-width: rTAB_ms_w)
  {
    height: 10vw
    grid-template-columns: auto
    grid-template-rows: 6vw 4vw
  }
  img
    width: auto
    height: log_ms_h
    z-index: -1
    @media screen and (max-width: rTAB_ms_w)
    {
      height: logTAB_ms_h
    }
    @media screen and (max-width: rSM_ms_w)
    {
      height: logSM_ms_h
    }
  ul
    margin 0
    padding: 0
    grid-column-start: 2
    grid-column-end: 3
    display: grid
    grid-template-columns: auto auto auto auto
    grid-template-rows: auto
    grid-gap: 1vw
    justify-content: center
    align-items: center
    li
      display: grid
      @media screen and (max-width: rSM_ms_w)
      {
        justify-content center
      }
    a
      color white
      text-decoration: none
      font-size: Mfont_ms
      @media screen and (max-width: rTAB_ms_w)
      {
        font-size: MfontTAB_ms
      }
      @media screen and (max-width: rSM_ms_w) and (orientation: portrait)
      {
        font-size: MfontSM_ms * 2
      }
      @media screen and (max-width: rSM_ms_w) and (orientation: landscape)
      {
        font-size: MfontSM_ms
      }
      @media screen and (max-width: rSM_ms_w) and (min-aspect-ratio: 840/662)
      {
        font-size: MfontSM_ms * 1.3
      }

    @media screen and (max-width: rTAB_ms_w)
    {
      grid-column-start: 1
      grid-column-end: 2
      grid-row-start: 2
      grid-row-end: 3
      grid-gap: 10vw
    }
    @media screen and (max-width: rSM_ms_w) and (orientation: portrait)
    {
      padding-top 12vw
      grid-template-columns auto
      grid-template-rows auto auto auto auto
      align-content center
      grid-gap: 10vh
      z-index -3
      top -110vh
    }
    @media screen and (max-width: rSM_ms_w) and (orientation: landscape)
    {
      padding-top 12vw
      grid-template-columns auto auto auto auto
      grid-template-rows auto
      align-content center
      grid-gap: 5vw
      z-index -3
      top -110vh
    }

.log_ms_01
  display: grid
  position: fixed
  margin: 0
  padding: 0
  width: romlog_ms_w
  height: romlog_ms_h
  top: -3.6vw
  left: 8vw
  background-image: url('../images/logo_rombos.png')
  background-position: center
  background-size: auto romlog_ms_h
  background-repeat: no-repeat
  justify-content: center
  align-items: center
  z-index: -2
  @media screen and (max-width: rTAB_ms_w)
  {
    padding: 0
    left: 0
    width: 100%
    height: romlogTAB_ms_h
    background-size: auto romlogTAB_ms_h
    top: -4.4vw
  }
  @media screen and (max-width: rSM_ms_w)
  {
    padding: 0
    left: 0
    width: 100%
    height: romlogSM_ms_h
    background-size: auto romlogSM_ms_h
    top: -8vw
  }
.rombos_ms_01
  position: fixed
  width: 100%
  height: rombos_ms_h
  top: rombos_ms_t
  background-image: url('../images/Rombos.png')
  background-size: auto rombos_ms_h
  z-index: -3
  @media screen and (max-width: rTAB_ms_w)
  {
    height: rombosTAB_ms_h
    top: rombosTAB_ms_t
    background-size: auto rombosTAB_ms_h
  }
  @media screen and (max-width: rSM_ms_w)
  {
    height: rombosSM_ms_h
    top: rombosSM_ms_t
    background-size: auto rombosSM_ms_h
  }
.menu_ms_02
  @media screen and (max-width: rSM_ms_w)
  {
    background-color rgb(3,3,3,0.8)
    position fixed
    width 100%
    height 100%
    transition 1000ms
  }
  &.active
    @media screen and (max-width: rSM_ms_w)
    {
      background-color rgb(53,53,53)
      top 0
    }
