@import "_variable.styl"

*
  // border: 0.03vw solid red
  font-family: 'Open Sans', sans-serif
body
    margin: 0
    display: grid
    justify-content: center
    align-items: center
.bmenu_ms_01
  display: none
.menu_ms_01
  width: 100%
  position: fixed
  height: 4vw
  top: 0
  background-color: rgba(0.3,0.3,0.3,0.8)
  margin: 0
  display: grid
  grid-template-columns: logo_ms menu_ms
  grid-template-rows: auto
  @media screen and (max-width: rTAB_ms_w)
  {
    height: 10vw
    grid-template-columns: auto
    grid-template-rows: 6vw 4vw
  }
  img
    width: auto
    height: log_ms_h
    z-index: -1
