    {
      height: logTAB_ms_h
    }
  ul
    margin 0
    padding: 0
    grid-column-start: 2
    grid-column-end: 3
    display: grid
    grid-template-columns: auto auto auto auto
    grid-template-rows: auto
    grid-gap: 1vw
    justify-content: center
    align-items: center
    li
      display: grid
    a
      color white
      text-decoration: none
      font-size: 1.2vw
      @media screen and (max-width: rTAB_ms_w)
      {
        font-size: 1.6vw
      }
    @media screen and (max-width: rTAB_ms_w)
    {
      grid-column-start: 1
      grid-column-end: 2
      grid-row-start: 2
      grid-row-end: 3
      grid-gap: 10vw
    }
