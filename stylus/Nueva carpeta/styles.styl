  display: grid
  position: fixed
  margin: 0
  padding: 0
  width: romlog_ms_w
  height: romlog_ms_h
  top: -3.6vw
  left: 8vw
  background-image: url('../images/logo_rombos.png')
  background-position: center
  background-size: auto romlog_ms_h
  background-repeat: no-repeat
  justify-content: center
  align-items: center
  z-index: -2
  @media screen and (max-width: rTAB_ms_w)
