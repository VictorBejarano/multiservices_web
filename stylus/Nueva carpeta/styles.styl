.contact_us
  padding: fpadd4k_ms
  background-clip: content-box
  width: r4k_ms_w - (2 * fpadd4k_ms)
  height: r4k_ms_h - (2 * fpadd4k_ms)
  display: grid
  grid-template-columns: auto auto auto auto
  grid-template-rows: auto (r4k_ms_w * 0.12) (r4k_ms_w * 0.155) (r4k_ms_w * 0.02) auto
  justify-content: center
  grid-gap: gap4k_ms
